{
  "name": "@sitespeed.io/log",
  "description": "The log for sitespeed.io projects",
  "version": "0.1.0",
  "type": "module",
  "devDependencies": {
    "ava": "6.2.0",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-unicorn": "56.0.1",
    "prettier": "3.4.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "src/**/*"
  ],
  "exports": {
    ".": {
      "import": "./src/logger.js"
    }
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "Peter Hedenskog",
  "repository": {
    "type": "git",
    "url": "https://github.com/sitespeedio/log.git"
  },
  "license": "Apache-2.0",
  "ava": {
    "files": [
      "test/**/*"
    ],
    "concurrency": 1,
    "failFast": true,
    "nodeArguments": [
      "--trace-deprecation"
    ]
  }
}
